git init

touch README.md

git add .

git commit -m "first commit"

git branch -M main

git remote add origin git@github.com:Menna97/lab2_vc.git

git push origin main

git branch dev

git checkout dev

git status

touch steps

gedit steps

touch MennaElGedawy

git checkout main

git branch test

git checkout main

git merge dev

git merge test

git push origin dev

git push origin test

to delete remote branch:

git push origin :branch_name

to delete local branch:

git branch -d branch_name

git branch -D branch_name

git tag -a v1.4 -m "version 1.4"

git push origin v1.4

to list tags locally:

git tag

to delete local tag:

git tag -d v1.4

to delete remote tag:

git push origin --delete v1.4
